import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String str1 = sc.nextLine();
        String str2 = sc.nextLine();
        int i= sc.nextInt();
        int j= sc.nextInt();
        StringBuilder sb = new StringBuilder(str1);
        sb.replace(i,j,str2);
        {
            System.out.println(sb);
        }
    }
}
